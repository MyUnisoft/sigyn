{
  "name": "@sigyn/slack",
  "version": "1.6.1",
  "description": "Send Sigyn alerts to Slack via webhook",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "glob -c \"tsx --test\" \"./test/**/*.spec.ts\"",
    "coverage": "c8 -r html npm test",
    "lint": "cross-env eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyUnisoft/sigyn.git",
    "directory": "src/slack"
  },
  "bugs": {
    "url": "https://github.com/MyUnisoft/sigyn/issues"
  },
  "homepage": "https://github.com/MyUnisoft/sigyn/blob/main/src/slack/README.md",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@myunisoft/httpie": "^2.0.1",
    "pupa": "^3.1.0"
  }
}
